{
  "name": "Debug Rust (Remote)",
  "type": "cppdbg",
  "request": "launch",
  "program": "${workspaceFolder}/target/debug/rust-mysql-sqx-api", // 本地符号文件路径
  "args": [],
  "cwd": "${workspaceFolder}",
  "MIMode": "gdb",
  "stopAtEntry": false,
  "preLaunchTask": "cargo build",
  "miDebuggerPath": "/usr/bin/gdb", // 本地 gdb 路径
  "miDebuggerServerAddress": "192.168.229.129:1234", // 远程 gdbserver 地址
  "setupCommands": [
    {
      "description": "Enable pretty-printing for gdb",
      "text": "-enable-pretty-printing",
      "ignoreFailures": true
    }
  ],
  "logging": {
    "engineLogging": true, // 启用调试器引擎日志
    "trace": true,
    "traceResponse": true
  },
  "sourceLanguages": [
    "rust"
  ]
}